{
  "name": "spacexinterop.client",
  "version": "0.2.2-beta",
  "scripts": {
    "ng": "ng",
    "start": "node -e \"process.env.SSL_CERT_FILE=require('path').join(process.env.HOME || '', '.aspnet', 'https', process.env.npm_package_name+'.pem'); process.env.SSL_KEY_FILE=require('path').join(process.env.HOME || '', '.aspnet', 'https', process.env.npm_package_name+'.key');\" && ng serve --ssl --ssl-cert $SSL_CERT_FILE --ssl-key $SSL_KEY_FILE --host=localhost",
    "prestart": "node aspnetcore-https.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/material": "^20.2.1",
    "@angular/platform-browser": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/service-worker": "^20.2.0",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
