{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MainConnection": "Server=(localdb)\\ProjectModels;Database=SpaceXInteropDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "RateLimiter": {
    "PermitLimit": 10,
    "WindowSeconds": 10
  },
  "ExternalApis": {
    "SpaceXApi": {
      "BaseUrl": "https://api.spacexdata.com/v5/"
    }
  },
  "Environment": "Development",
  "AllowedOrigins": [
    "http://localhost:4200",
    "https://localhost:4200"
  ]
}